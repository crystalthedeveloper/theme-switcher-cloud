{
  "name": "theme-switcher-cloud",
  "main": "src/_worker.js",
  "compatibility_date": "2024-01-01",

  "assets": {
    "binding": "R2",
    "directory": "./",
    "serve": true
  },

  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "my-bucket"
    }
  ],

  "routes": [
    {
      "pattern": "/theme-switcher.js"
    }
  ]
}
